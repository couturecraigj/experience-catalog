{
  "extends": ["wesbos", "react-app"],
  "rules": {
    "no-console": 2,
    "quotes": ["error", "double", { "avoidEscape": true }],
    "react/prop-types": ["warn"],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}
